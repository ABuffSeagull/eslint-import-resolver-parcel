{
	"name": "eslint-import-resolver-parcel",
	"version": "1.10.0",
	"description": "Parcel import resolution plugin for eslint-plugin-import.",
	"homepage": "https://github.com/ABuffSeagull/eslint-import-resolver-parcel",
	"bugs": "https://github.com/ABuffSeagull/eslint-import-resolver-parcel/issues",
	"repository": "github:abuffseagull/eslint-import-resolver-parcel",
	"main": "src/index.js",
	"author": "ABuffSeagull <reecevanatta@gmail.com>",
	"license": "MIT",
	"keywords": [
		"parceljs",
		"parcel-bundler",
		"eslint",
		"eslint-import-resolver"
	],
	"scripts": {
		"lint": "eslint --ext .js src/index.js test/ --fix",
		"test": "jest",
		"stryker": "stryker run",
		"version": "npx @pika/pack build"
	},
	"devDependencies": {
		"@abuffseagull/eslint-config-node": "^4.0.0",
		"@babel/core": "^7.4.4",
		"@babel/preset-env": "^7.4.4",
		"@iarna/toml": "^2.2.1",
		"@pika/plugin-build-node": "^0.6.0",
		"@pika/plugin-standard-pkg": "^0.6.0",
		"@pika/types": "^0.6.0",
		"@stryker-mutator/core": "^2.0.2",
		"@stryker-mutator/html-reporter": "^2.0.2",
		"@stryker-mutator/javascript-mutator": "^2.0.2",
		"@stryker-mutator/jest-runner": "^2.0.2",
		"babel-jest": "^24.8.0",
		"eslint": "^6.1.0",
		"eslint-formatter-kakoune": "^1.0.0",
		"eslint-plugin-jest": "^22.13.7",
		"jest": "^24.8.0"
	},
	"dependencies": {
		"resolve": "^1.11.1"
	},
	"alias": {
		"folder-alias": "./test-folder",
		"test-alias": "jest",
		"foo": "bar",
		"foobar": "jest"
	},
	"resolutions": {
		"lodash": "^4.17.13",
		"lodash.defaultsdeep": "^4.6.1"
	},
	"@pika/pack": {
		"pipeline": [
			[
				"@pika/plugin-standard-pkg"
			],
			[
				"@pika/plugin-build-node"
			]
		]
	}
}
